function StatisticsComponent_div_0_Template(rf, ctx) { if (rf & 1) {
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵelementStart"](0, "div", 1);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵelement"](1, "canvas", 2);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵelementEnd"]();
} if (rf & 2) {
    const ctx_r0 = _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵnextContext"]();
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵadvance"](1);
    _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵproperty"]("data", ctx_r0.pieChartData)("labels", ctx_r0.pieChartLabels)("chartType", ctx_r0.pieChartType);
} }
class StatisticsComponent {
    constructor(statisticSer) {
        this.statisticSer = statisticSer;
        this.bussinesArr = [];
        this.pieChartLabels = [];
        this.pieChartData = [];
        this.pieChartType = 'pie';
        this.statisticsArr = [];
    }
    pushFunction() {
        for (let s of this.statisticsArr) {
            this.pieChartLabels.push(s.CategoryName);
            this.pieChartData.push(s.Data);
        }
    }
    ngOnInit() {
        this.userCode = Number(sessionStorage.getItem("UserCode"));
        this.statisticSer.getTimeAvgByCategory(this.userCode).subscribe(suc => { this.statisticsArr = suc, this.pushFunction(); }, err => { console.log("error get statistics list"); });
