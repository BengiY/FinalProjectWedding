class EventWindowComponent {
    constructor(distanceSer, OpenHourSer, modal, router, EventSer, BusinessSer, CategorySer, ScheduleSer) {
        this.distanceSer = distanceSer;
        this.OpenHourSer = OpenHourSer;
        this.modal = modal;
        this.router = router;
        this.EventSer = EventSer;
        this.BusinessSer = BusinessSer;
        this.CategorySer = CategorySer;
        this.ScheduleSer = ScheduleSer;
        this.events = [...src_app_sample_events__WEBPACK_IMPORTED_MODULE_6__["SAMPLE_EVENTS"]];
        this.allSchedule = [...src_app_sample_events__WEBPACK_IMPORTED_MODULE_6__["ALL_SCHEDULER"]];
        this.viewTo = false;
        this.currentEvent = new _class_event__WEBPACK_IMPORTED_MODULE_8__["event"]();
        this.newSchedule = new src_app_class_schedule__WEBPACK_IMPORTED_MODULE_5__["schedule"]();
        //array by category
        this.arrBusiness = [];
        this.selectedBusiness = new src_app_class_business__WEBPACK_IMPORTED_MODULE_4__["business"]();
        this.arrCategory = [];
        this.viewBusiness = false;
        this.FullCategory = new _class_category__WEBPACK_IMPORTED_MODULE_7__["category"]();
        this.inputValue = "";
        this.nextSchedule = false;
        this.viewDatatToEdit = new src_app_class_schedule__WEBPACK_IMPORTED_MODULE_5__["schedule"]();
        this.viewChooseBussinessAndCategory = false;
        this.group = true;
        this.arrAllBusiness = [];
        this.openHourArr = [];
        this.viewBusinessDetails = false;
        this.resultHour = new src_app_class_schedule__WEBPACK_IMPORTED_MODULE_5__["schedule"]();
        this.display = "none";
    }
    getDataToShow() {
        this.group = false;
        src_app_sample_events__WEBPACK_IMPORTED_MODULE_6__["ALL_SCHEDULER"].forEach(element => {
            if (element.ScheduleId == this.model.data.id) {
                this.viewDatatToEdit = element;
                return false;
            }
        });
        // console.log(this.viewDatatToEdit)
        this.inputValue = this.viewDatatToEdit.Description;
        this.arrAllBusiness.forEach(element => {
            if (element.BusinessCode == this.viewDatatToEdit.BusinessCode) {
                this.business = element.BusinessName;
                this.category = element.CategoryCode.toString();
                return false;
            }
        });
        this.arrCategory.forEach(element => {
            if (element.CategoryCode == Number(this.category)) {
                this.category = element.CategoryName;
                return false;
            }
        });
        console.log(this.category + " " + this.business);
        this.viewChooseBussinessAndCategory = true;
    }
    ngOnInit() {
        this.CategorySer.getAllCategory().subscribe(suc => { this.arrCategory = suc; }, err => { alert("errrrrror category"); console.log(err); });
        this.BusinessSer.getAllBusniess().subscribe(suc => { this.arrAllBusiness = suc; if (this.model.title != "") {
            this.getDataToShow();
        } }, err => { alert("eror get all businnes"); console.log(err); });
        if (!this.model) {
            this.model = new axiom_scheduler__WEBPACK_IMPORTED_MODULE_2__["AxiomSchedulerEvent"]('', new Date(Date.now()), new Date(Date.now()), {});
        }
        if (this.model.from) {
            var from = moment__WEBPACK_IMPORTED_MODULE_3__(this.model.from);
            this.fromTime = from.format('HH:mm');
        }
        if (this.model.to) {
            var to = moment__WEBPACK_IMPORTED_MODULE_3__(this.model.to);
            this.toTime = to.format('HH:mm');
        }
        //
        this.form = new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormGroup"]({
            subject: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]("", _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            taskTime: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]("", _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            category: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]("", _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            business: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]("", _angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required),
            note: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](""),
            fromDate: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](""),
            toDate: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](""),
            description: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"](""),
            color: new _angular_forms__WEBPACK_IMPORTED_MODULE_1__["FormControl"]("")
        });
    }
    showGroup() {
        this.group = true;
        this.viewChooseBussinessAndCategory = false;
    }
    onSelectedCategory(event) {
        this.BusinessSer.getBusinessByCategoryCode(event).subscribe(suc => { this.arrBusiness = suc; this.viewBusiness = true, this.selectedCategory = event; }, err => { alert("error to load a business list"); console.log(err); });
    }
    onKey(event) {
        this.inputValue = event.target.value;
    }
    checkLocation(getdistance) {
        this.distanceSer.calcDistance(getdistance).subscribe(suc => { console.log(suc), this.resultDistance = suc; return this.resultDistance; }, err => { console.log("errror get distance"); });
    }
    checkHours(date, hour, bussinesCode) {
        // this.dateInString=date;
        var currentB;
        var beforeB;
        for (let x of src_app_sample_events__WEBPACK_IMPORTED_MODULE_6__["ALL_SCHEDULER"]) {
            var d = new Date(x.EventDate);
            if (d.getDate() == date.getDate()) {
                if (x.Hour.toString().slice(0, 2) == hour.toString().slice(0, 2)) {
                    this.beforeHour = x.Hour;
                    for (let b of this.arrAllBusiness) {
                        if (Number(bussinesCode) == b.BusinessCode) {
                            currentB = b.FullAddress;
                        }
                        if (x.BusinessCode == b.BusinessCode) {
                            beforeB = b.FullAddress;
                        }
                    }
                    var distanceByMinutes = this.checkLocation(new src_app_class_distance__WEBPACK_IMPORTED_MODULE_9__["distance"](beforeB, currentB));
                    ///change
                    return 50;
                }
                if (x.Hour == hour) {
                    return x;
                }
            }
        }
        return null;
    }
    // checkHoursOfBusiness(date)
    // {
    //   var day=date.getDay();
