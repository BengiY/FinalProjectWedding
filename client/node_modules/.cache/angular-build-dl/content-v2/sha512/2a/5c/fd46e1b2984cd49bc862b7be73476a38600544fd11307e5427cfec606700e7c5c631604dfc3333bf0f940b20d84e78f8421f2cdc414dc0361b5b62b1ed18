            sessionStorage.setItem("userInSystem", "0");
        }
        this.model.step = 5;
        this.model.toolbar = true;
        this.model.eventTemplate = true;
        this.model.theme = 'light';
        this.model.locale = true;
        // this.model.AxiomSchedulerView="Month"
