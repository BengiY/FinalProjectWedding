    constructor(injector) {
        this.injector = injector;
        this.events = [...src_app_sample_events__WEBPACK_IMPORTED_MODULE_0__["SAMPLE_EVENTS"]];
        this._modalService = this.injector.get(_ng_bootstrap_ng_bootstrap__WEBPACK_IMPORTED_MODULE_2__["NgbModal"]);
    }
    editEvent($event = null) {
        var instance = this._modalService.open(src_app_components_event_window_event_window_component__WEBPACK_IMPORTED_MODULE_1__["EventWindowComponent"], { size: 'sm', centered: true });
        if ($event) {
            instance.componentInstance.model = Object.assign({}, $event);
        }
        instance.result.then((model) => {
            if (model) {
                if ($event) {
                    var index = this.events.findIndex(e => e._id === $event._id);
                    if (index > -1) {
                        this.events[index] = model;
                        this.refreshView();
                    }
                }
                else {
                    this.events.push(model);
                    this.refreshView();
                }
            }
        });
    }
    removeEvent($event) {
        var index = this.events.findIndex(e => e._id === $event._id);
        if (index > -1) {
            this.events.splice(index, 1);
            this.refreshView();
        }
    }
    refreshView() { }
}


